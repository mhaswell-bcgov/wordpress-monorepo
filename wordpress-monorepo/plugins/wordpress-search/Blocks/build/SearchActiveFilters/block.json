{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wordpress-search/search-active-filters",
  "version": "1.0.0",
  "title": "Search Active Filters",
  "category": "search",
  "icon": "filter",
  "description": "A block that displays active search filters as removable chips",
  "attributes": {
    "showCount": {
      "type": "boolean",
      "default": true
    },
    "showClearAll": {
      "type": "boolean",
      "default": true
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "viewStyle": "file:./view.css",
  "viewScript": "file:./view.js",
  "render": "file:./render.php",
  "style": "file:./view.css",
  "dependencies": [
    "wp-api-fetch",
    "wp-url",
    "wp-components",
    "wp-element",
    "wp-i18n"
  ]
}